{
  "name": "Bookmark",
  "short_name": "Bookmark",
  "version": "0.0.1",
  "homepage_url": "https://github.com/jimmy-zhening-luo/bookmark",
  "incognito": "spanning",
  "icons": {
    "16": "static/icons/icon-16.png",
    "32": "static/icons/icon-32.png",
